{
  "name": "tailwind-project",
  "version": "1.0.0",
  "description": "Tailwind CSS project setup",
  "main": "index.js",
  "scripts": {
    "build:css": "tailwindcss -i ./src/input.css -o ./dist/output.css --minify",
    "watch:css": "tailwindcss -i ./src/input.css -o ./dist/output.css --watch",
    "start": "npm run watch:css"
  },
  "dependencies": {
    "mupdf-webviewer": "^0.11.0"
  },
  "devDependencies": {
    "tailwindcss": "^3.3.3",
    "autoprefixer": "^10.4.0",
    "postcss": "^8.4.0"
  }
}
